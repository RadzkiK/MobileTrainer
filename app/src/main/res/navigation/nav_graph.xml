<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/home2">
    <fragment
        android:id="@+id/training"
        android:name="com.engineer.mobiletrainer.Training"
        android:label="fragment_training"
        tools:layout="@layout/fragment_training" />
    <fragment
        android:id="@+id/exerciseChoice"
        android:name="com.engineer.mobiletrainer.ExerciseChoice"
        android:label="fragment_exercise_choice"
        tools:layout="@layout/fragment_exercise_choice" >
        <action
            android:id="@+id/action_exerciseChoice_to_trainingActivity"
            app:destination="@id/trainingActivity" />
        <action
            android:id="@+id/action_exerciseChoice_to_trainingActivity2"
            app:destination="@id/trainingActivity" />
    </fragment>
    <fragment
        android:id="@+id/home2"
        android:name="com.engineer.mobiletrainer.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home2_to_mainPage"
            app:destination="@id/mainPage" />
    </fragment>
    <fragment
        android:id="@+id/settings2"
        android:name="com.engineer.mobiletrainer.AppSettings"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <activity
        android:id="@+id/trainingActivity"
        android:name="com.engineer.mobiletrainer.TrainingActivity"
        android:label="fragment_training"
        tools:layout="@layout/fragment_training" /><action android:id="@+id/action_global_settings2" app:destination="@id/settings2"/>
    <fragment
        android:id="@+id/mainPage"
        android:name="com.engineer.mobiletrainer.MainPage"
        android:label="fragment_main_page"
        tools:layout="@layout/fragment_main_page" >
        <action
            android:id="@+id/action_mainPage_to_exerciseChoice"
            app:destination="@id/exerciseChoice" />
        <action
            android:id="@+id/action_mainPage_to_plansFragment"
            app:destination="@id/plansFragment" />
    </fragment>
    <fragment
        android:id="@+id/profile"
        android:name="com.engineer.mobiletrainer.ProfilePage"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" /><action android:id="@+id/action_global_profile" app:destination="@id/profile"/><action android:id="@+id/action_global_mainPage" app:destination="@id/mainPage"/>
    <fragment
        android:id="@+id/plansFragment"
        android:name="com.engineer.mobiletrainer.PlansFragment"
        android:label="fragment_plans"
        tools:layout="@layout/fragment_plans" />
</navigation>